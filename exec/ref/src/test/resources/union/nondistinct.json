{
    "head": {
        "type": "APACHE_DRILL_LOGICAL",
        "version": 1,
        "generator": {
            "type": "manual",
            "info": "na"
        }
    },
    "storage": {
        "queue": {
            "type": "queue",
            "encoding": "RECORD"
        },
        "donuts-json": {
            "type": "classpath"
        }
    },
    "query": [
        {
            "op": "scan",
            "@id": 1,
            "memo": "initial_scan",
            "storageengine": "donuts-json",
            "selection": {
                "path": "/employees.json",
                "type": "JSON"
            },
            "ref": "_MAP"
        },
        {
            "op": "project",
            "input": 1,
            "@id": 2,
            "projections": [
                {
                    "ref": "output.deptId",
                    "expr": "_MAP.deptId"
                }
            ]
        },
        {
            "op": "scan",
            "@id": 3,
            "memo": "initial_scan",
            "storageengine": "donuts-json",
            "selection": {
                "path": "/departments.json",
                "type": "JSON"
            },
            "ref": "_MAP"
        },
        {
            "op": "project",
            "input": 3,
            "@id": 4,
            "projections": [
                {
                    "ref": "output.deptId",
                    "expr": "_MAP.deptId"
                }
            ]
        },
        {
            "op": "union",
            "@id": 5,
            "distinct": false,
            "inputs": [2, 4]
        },
        {
            "op": "store",
            "input": 5,
            "@id": 6,
            "memo": "output sink",
            "target": {
                "number": 0
            },
            "partition": null,
            "storageEngine": "queue"
        }
    ]
}
