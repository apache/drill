{
  head:{
    type:"APACHE_DRILL_LOGICAL",
    version:"1",
    generator:{
      type:"manual",
      info:"na"
    }
  },
  storage:{
    "fs" :
      {
        "type":"file",
        "connection" : "file:///"
      }
  },
  query:[
    {
      @id:"1",
      op:"scan",
      memo:"initial_scan",
      storageengine:"fs",
      selection: {
        format: {type: "parquet"},
        files: [&REPLACED_IN_PARQUET_TEST&]
      }
    },
    {
      @id:"2",
      input: 1,
      op: "store",
      memo: "output sink",
      target: {
        file: "console:///stdout"
      }

    }

  ]
}
