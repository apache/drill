{
  head : {
    version : 1,
    generator : {
      type : "optiq",
      info : "na"
    },
    type : "APACHE_DRILL_PHYSICAL"
  },
  graph : [ {
    "pop" : "parquet-scan",
    "@id" : 1,
    "entries" : [ {
      "path" : "tpch/nation.parquet"
    } ],
    "storage" : {
      "type" : "file",
      "connection" : "classpath:///"
    },
    "format" : {
      "type" : "parquet"
    }
  }, {
    pop : "project",
    @id : 2,
    exprs : [ {
      ref : "$f0",
      expr : "N_REGIONKEY"
    }, {
      ref : "$f1",
      expr : "N_NATIONKEY"
    }, {
      ref : "$f2",
      expr : "N_NAME"
    } ],
    child : 1
  }, {
    pop : "hash-aggregate",
    @id : 3,
    child : 2,
    keys : [ {
      ref : "$f0",
      expr : "$f0"
    }, {
      ref : "$f2",
      expr : "$f2"
    } ],
    exprs : [ {
      ref : "Y",
      expr : "sum($f1) "
    } ]
  }, {
    pop : "screen",
    @id : 4,
    child : 3
  } ]
}
