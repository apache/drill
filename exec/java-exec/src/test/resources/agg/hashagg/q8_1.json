{
  head : {
    version : 1,
    generator : {
      type : "optiq",
      info : "na"
    },
    type : "APACHE_DRILL_PHYSICAL"
  },
  graph : [ {
    "pop" : "parquet-scan",
    "@id" : 1,
    "entries" : [ {
      "path" : "tpch/orders.parquet"
    } ],
    "storage" : {
      "type" : "file",
      "connection" : "classpath:///"
    },
    "format" : {
      "type" : "parquet"
    }
  }, {
    pop : "project",
    @id : 2,
    exprs : [ {
      ref : "$f0",
      expr : "O_CUSTKEY"
    }, {
      ref : "$f1",
      expr : "O_ORDERKEY"
    } ],
    child : 1
  }, {
    pop : "hash-aggregate",
    @id : 3,
    child : 2,
    keys : [ {
      ref : "$f0",
      expr : "$f0"
    } ],
    exprs : [ {
      ref : "Y",
      expr : "sum($f1) "
    } ]
  }, {
   pop : "sort",
    @id : 4,
    child : 3,
    orderings : [ {
      order : "ASC",
      expr : "$f0"
    }, {
      order : "ASC",
      expr : "Y"
    } ],
    reverse : false
  }, {
    pop : "selection-vector-remover",
    @id : 5,
    child : 4
  }, {
    pop: "limit",
    @id : 6,
    child: 5,
    first: 0,
    last: 100
  }, {
    pop : "screen",
    @id : 7,
    child : 6
  } ]
}
