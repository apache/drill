{
  "head" : {
    "type" : "APACHE_DRILL_LOGICAL",
    "version" : 1,
    "generator" : {
      "type" : "manual",
      "info" : "na"
    }
  },
  "storage" : {
    "queue" : {
        "type" : "queue",
        "encoding" : "RECORD"
    },
    "donuts-json" : {
          "type": "classpath"
    }
  },

  "query" : [ {
    "op" : "scan",
    "@id" : 1,
    "memo" : "initial_scan",
    "storageengine" : "donuts-json",
    "selection" : {
      "path" : "/employees.json",
      "type" : "JSON"
    },
    "ref" : "_MAP"
  }, {
    "op" : "project",
    "input" : 1,
    "@id" : 2,
    "projections" : [ {
      "ref" : "output.deptId",
      "expr" : "_MAP.deptId"
    } ]
  },  {
    op: "segment",
    "input" : 2,
    "@id" : 3,
    ref: "segment",
    exprs: ["deptId"]
  }, {
    "input" : 3,
    "@id" : 4,
    op: "collapsingaggregate",
    within: "segment",
    carryovers: [ "deptId" ],
    aggregations: [
                { ref: "typeCount",  expr: "count(1)" }
              ]
  }, 
  {
    op: "order",
    "input" : 4,
    "@id" : 5,
    orderings: [
    	{order: "asc", expr: "deptId"}
    ]
  },
  {
    "op" : "store",
    "input" : 5,
    "@id" : 6,
    "memo" : "output sink",
    "target" : {
      "number" : 0
    },
    "partition" : null,
    "storageEngine" : "queue"
  } ]
}
